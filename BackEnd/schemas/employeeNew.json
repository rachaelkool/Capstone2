{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/employeeNew.json",
    "type": "object",
    "properties": {
      "empId": {
        "type": "integer",
        "minLength": 3,
        "maxLength": 30
      },
      "password": {
        "type": "string",
        "minLength": 5,
        "maxLength": 20
      },
      "firstName": {
        "type": "string",
        "minLength": 1,
        "maxLength": 30
      },
      "lastName": {
        "type": "string",
        "minLength": 1,
        "maxLength": 30
      },
      "isAdmin": {
        "type": "boolean"
      }
    },
    "additionalProperties": false,
    "required": [
      "empId",
      "firstName",
      "lastName",
      "password"
    ]
  }